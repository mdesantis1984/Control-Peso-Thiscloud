@using MudBlazor

<MudDialog>
    <DialogContent>
        <MudText Typo="Typo.body1">@ContentText</MudText>
    </DialogContent>
    <DialogActions>
        <MudButton OnClick="Cancel">Cancelar</MudButton>
        <MudButton Color="@Color" Variant="Variant.Filled" OnClick="Confirm">
            @ButtonText
        </MudButton>
    </DialogActions>
</MudDialog>
